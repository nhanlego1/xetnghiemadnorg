<wpml-config>
    <custom-types>
        <custom-type translate="1">doctor</custom-type>
        <custom-type translate="1">faq</custom-type>
        <custom-type translate="1">gallery-item</custom-type>
        <custom-type translate="1">service</custom-type>
        <custom-type translate="1">slide</custom-type>
        <custom-type translate="1">testimonial</custom-type>
