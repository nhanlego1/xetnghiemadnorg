        <key name="theme_service_title" />
        <key name="theme_service_text" />
        <key name="theme_gallery_title" />
        <key name="theme_gallery_text" />
        <key name="theme_postal_address" />
        <key name="theme_contact_form_heading" />
        <key name="theme_contact_form_text" />
        <key name="theme_about_text" />
        <key name="theme_copyright_text" />
